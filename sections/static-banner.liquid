<style></style>

<main class="tw-bg-white">
  <section class=" tw-mx-auto tw-grid tw-content-center tw-gap-8 tw-grid-cols-3 tw-p-4 md:tw-p-0 md:tw-py-[7rem] tw-font-bold tw-font-heading tw-w-lg">
    <div class="tw-mx-auto">
      <img class="tw-rounded-lg" loading="lazy" height="auto" width="160" src="{{ 'icon-field.jpeg' | asset_url }}">
    </div>
    <div class="tw-mx-auto">
      <img class="tw-rounded-lg" loading="lazy" height="auto" width="160" src="{{ 'icon-office.jpeg' | asset_url }}">
    </div>
    <div class="tw-mx-auto">
      <img
        class="tw-rounded-lg"
        loading="lazy"
        height="auto"
        width="160"
        src="{{ 'icon-tailgate.jpeg' | asset_url }}"
      >
    </div>
  </section>
</main>

{% schema %}
{
  "name": "Static Banner",
  "class": "static-banner-section",
  "settings": [
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "static-banner-section",
      "default": "accent-1"
    }
  ],
  "presets": [
    {
      "name": "Static Banner"
    }
  ]
}
{% endschema %}
